<%= form_for @publisher do |f| %>
  <%= "Please add your information to your account." %>
  <br>
  <br>
  <%= f.label :username %>
  <%=f.text_field :username %>
  <br>
  <%=f.label :first_name %>
  <%=f.text_field :first_name %>
  <br>
  <%=f.label :last_name %>
  <%=f.text_field :last_name %>
  <br>
  <%=f.label :service_group %>
  <%=f.number_field :service_group %>
  <br>
  <br>
  <%=f.collection_check_boxes(:congregation_id, Congregation.all, :id, :name) %>
  <br>
  <br>
  <%= f.submit "Link and Add Publisher to your Email"%>
<%end%>
